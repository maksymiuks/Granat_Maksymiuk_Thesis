create_html_table <- function(data_row){
as.character(htmlTable(
data_row,
css.cell = rbind(rep("border-collapse: collapse;
margin: 25px 0;
font-size: 0.9em;
padding-left: .5em;
padding-right: .5em;
font-family: sans-serif;
box-shadow: 0 0 5px rgba(0, 0, 0, 0.15);
",
times = ncol(data_row)),
rep("border-collapse: collapse;
margin: 25px 0;
font-size: 0.9em;
padding-left: .5em;
padding-right: .5em;
font-family: sans-serif;
",
times = ncol(data_row))))
